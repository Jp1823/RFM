node n1 {
    type router
    model host
    network-config {
	hostname SERVER
	!
	interface eth0
	 ip address 200.0.6.30/24
	!
    }
    canvas c1
    iconcoords {844.0 432.0}
    labelcoords {847.0 477.0}
    interface-peer {eth0 n2}
}

