link l1 {
    delay 100
    nodes {n1 n2}
    bandwidth 1000000000
}

link l2 {
    delay 100
    nodes {n2 n3}
    bandwidth 1000000000
}

link l3 {
    nodes {n3 n4}
    bandwidth 0
}

link l4 {
    nodes {n3 n5}
    bandwidth 0
}

link l5 {
