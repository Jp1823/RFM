}

link l8 {
    delay 10
    nodes {n7 n8}
    bandwidth 1000000000
}

annotation a1 {
    iconcoords {498.0 159.0 896.0 536.0}
    type oval
    label {Core Network}
    labelcolor black
    fontfamily {Arial}
    fontsize {12}
    color #e6e394
    width 0
    border black
    canvas c1
}

annotation a2 {
    iconcoords {268.0 288.0 495.0 390.0}
    type rectangle
    label {Rede Acesso}
    labelcolor black
    fontfamily {Arial}
    fontsize {12}
    color #c0f6c9
    width 0
    border black
    rad 25
    canvas c1
}

canvas c1 {
    name {Canvas1}
}

option global {
    interface_names no
    ip_addresses yes
    ipv6_addresses yes
    node_labels yes
    link_labels yes
    show_api no
    background_images no
    annotations yes
    grid yes
    traffic_start 0
}

option session {
}

